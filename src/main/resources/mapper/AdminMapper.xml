<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mapper.AdminMapper">



    <insert id="register" parameterType="com.entity.UserMsg" useGeneratedKeys="true" keyProperty="id" >

     insert  into t_person_msg (person_account,person_password,person_name,person_sex,person_phone,person_email,person_rank,person_state,person_points)values(#{personAccount},#{personPassword},#{personName},
     #{personSex},#{personPhone},#{personEmail},#{personRank},#{personState},#{personPoints})


 </insert>




</mapper>